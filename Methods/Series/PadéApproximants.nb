Cell["\<\
Suppose now that you have also computed the following asymptotic series for \
the same function.\
\>", "CodeText",ExpressionUUID->"c5595785-f5e3-4ee8-9dce-1021496ac0db"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"asymptotic", "[", "x_", "]"}], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", " ", "x"}]], "+", 
     FractionBox["1", 
      RowBox[{"4", " ", 
       SuperscriptBox["x", "2"]}]], "+", 
     FractionBox["3", 
      RowBox[{"8", " ", 
       SuperscriptBox["x", "3"]}]]}]}], ";"}], TraditionalForm]], "InputOnly",\
ExpressionUUID->"a7ecfad9-90b7-421a-b8c1-7095b48efd29"],

