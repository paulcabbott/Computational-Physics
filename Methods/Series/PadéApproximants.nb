Cell[TextData[{
 "The diagonal Pad\[EAcute] approximant then has ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "=", 
    RowBox[{"M", "=", "4"}]}], TraditionalForm]],ExpressionUUID->
  "c5d89eb3-897a-4788-badf-9382b9d86c7d"],
 ". "
}], "Text",ExpressionUUID->"f6d5b05a-9189-4491-8206-a0a0f6c604c0"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"n", "=", 
   RowBox[{"m", "=", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"(", 
      RowBox[{"j", "+", "k", "-", "1"}], ")"}]}]}]}], 
  TraditionalForm]], "Input",ExpressionUUID->"ddc2408e-fe15-4fc0-afc5-\
d8843191c98f"],

