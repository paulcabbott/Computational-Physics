Cell[CellGroupData[{

Cell["Rayleigh-Ritz variational method", "Section",
 CellChangeTimes->{
  3.4805881296572113`*^9},ExpressionUUID->"18e9237a-e2c4-481e-be66-\
c07d065ab4c7"],

Cell["Consider the one-dimensional (Schr\[ODoubleDot]dinger) equation", "Text",
 CellChangeTimes->{{3.41933095826372*^9, 3.419330964519734*^9}, {
  3.4193311284261837`*^9, 3.419331129218317*^9}, {3.480588139903343*^9, 
  3.48058814419388*^9}},ExpressionUUID->"585c5e6f-d84c-4896-8611-\
3ac19331267e"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "2"]}], 
      RowBox[{
       SuperscriptBox["\[Psi]", "\[DoublePrime]"], "(", "x", ")"}]}], " ", 
     "+", 
     RowBox[{
      RowBox[{"V", "(", "x", ")"}], 
      RowBox[{"\[Psi]", "(", "x", ")"}]}]}], "\[LongEqual]", 
    RowBox[{"\[ScriptCapitalE]", " ", 
     RowBox[{"\[Psi]", "(", "x", ")"}]}]}], ","}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{
  3.419330699081221*^9, 3.419332398570854*^9, {3.51100225038365*^9, 
   3.511002280710079*^9}, {3.511071574323601*^9, 3.5110715852754374`*^9}},
 TextAlignment->Center,ExpressionUUID->"49c0a2ad-699c-4d23-abd2-ea4ca06f7b87"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Psi]", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "9e25b655-de6c-4322-9165-e1b604581d5b"],
 " is the ",
 StyleBox["wavefunction",
  FontWeight->"Bold"],
 " (or eigenstate or eigenfunction), ",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "d29a2431-3a1d-41a0-abd0-0091aa7bb820"],
 " is the potential, and ",
 "\[ScriptCapitalE]",
 " is the energy (or eigenvalue)."
}], "Text",
 CellChangeTimes->{{3.419330751668695*^9, 3.419330797525941*^9}, {
   3.4193309798282137`*^9, 3.419330991570179*^9}, {3.4193310237809277`*^9, 
   3.419331081128289*^9}, {3.4193324097035227`*^9, 3.4193324614950867`*^9}, 
   3.511002283205174*^9},ExpressionUUID->"48616962-39cb-4576-8c97-\
3ce3ba96fbd1"],

Cell[CellGroupData[{

Cell[TextData[{
 StyleBox["Example: ",
  FontWeight->"Bold"],
 "one-dimensional quantum harmonic oscillator"
}], "Subsubsection",
 CellChangeTimes->{{3.4193324891103287`*^9, 3.419332498625246*^9}, {
  3.511001201374712*^9, 
  3.51100121029044*^9}},ExpressionUUID->"0768db7b-6c01-4d30-8f0d-\
9ca3b52fd2e2"],

Cell[TextData[{
 "The equation for the one-dimensional quantum harmonic oscillator with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"V", "(", "x", ")"}], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     SuperscriptBox["x", "2"]}]}], TraditionalForm]],ExpressionUUID->
  "1967802c-30c0-4650-bb6d-612c57b8106e"],
 " reads"
}], "Text",
 CellChangeTimes->{{3.4193324891103287`*^9, 3.419332498625246*^9}, {
  3.511001201374712*^9, 
  3.511001213849594*^9}},ExpressionUUID->"bda9a475-7356-4223-ad0c-\
5f599bf9a6fa"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "2"]}], 
      RowBox[{
       SuperscriptBox["\[Psi]", "\[DoublePrime]"], "(", "x", ")"}]}], " ", 
     "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["x", "2"], 
      RowBox[{"\[Psi]", "(", "x", ")"}]}]}], "\[LongEqual]", 
    RowBox[{"\[ScriptCapitalE]", " ", 
     RowBox[{"\[Psi]", "(", "x", ")"}]}]}], ","}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{
  3.511001219748282*^9, 3.511002289449295*^9, {3.5110716063650923`*^9, 
   3.511071615690627*^9}},ExpressionUUID->"236b22c0-9dd2-475a-977b-\
e5bc6003ab24"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", "\[Infinity]"}], "<", "x", "<", "\[Infinity]"}], 
   TraditionalForm]],ExpressionUUID->"35164a4b-1bfe-4d1e-919d-8951a77ffec9"],
 " and the ",
 StyleBox["normalisation condition",
  FontWeight->"Bold"],
 " is"
}], "Text",ExpressionUUID->"bf8ad254-fcc2-4f34-b9e0-18acf5f3fdc6"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
    RowBox[{
     SuperscriptBox[
      RowBox[{"\[Psi]", "(", "x", ")"}], "2"], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], "\[LongEqual]", "1."}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.4193307083933372`*^9, 3.419330713705161*^9}, {
  3.511001221289839*^9, 3.51100122223379*^9}, {3.511071618098762*^9, 
  3.5110716187704363`*^9}},
 TextAlignment->Center,ExpressionUUID->"4a756812-2b2b-44a5-8201-c5e5a012b27b"],

Cell[TextData[{
 "The boundary condition is that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Psi]", "(", "x", ")"}], "\[Rule]", "0"}], TraditionalForm]],
  ExpressionUUID->"64f148ad-a456-441a-8096-5500fbb1e220"],
 " as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "\[Rule]", 
    RowBox[{"\[PlusMinus]", "\[Infinity]"}]}], TraditionalForm]],
  ExpressionUUID->"77a29e49-bfe0-4ded-ad88-e68118ca8f37"],
 "."
}], "Text",
 CellChangeTimes->{{3.4193316705583067`*^9, 
  3.419331698350895*^9}},ExpressionUUID->"ab3b162f-b2f4-49ce-ac81-\
913f2c32005a"],

Cell["\<\
In this case, exact solution is possible. Here are the first four eigenstates \
with their corresponding energy eigenvalue:\
\>", "Text",
 CellChangeTimes->{{3.51100203711679*^9, 3.511002037332727*^9}, {
  3.5423405660448217`*^9, 
  3.5423405669711037`*^9}},ExpressionUUID->"8e411182-db66-491d-bfd9-\
847a46e106d9"],

Cell[TextData[Cell[BoxData[
 FormBox[
  TagBox[
   StyleBox[GridBox[{
      {"n", 
       RowBox[{
        SubscriptBox["\[Psi]", "n"], "(", "x", ")"}], 
       RowBox[{
        SubscriptBox["\[ScriptCapitalE]", "n"], "\[LongEqual]", 
        RowBox[{"n", "+", 
         FractionBox["1", "2"]}]}]},
      {"0", 
       RowBox[{
        SuperscriptBox["\[Pi]", 
         RowBox[{
          RowBox[{"-", "1"}], "/", "4"}]], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          FractionBox[
           SuperscriptBox["x", "2"], "2"]}]]}], 
       FractionBox["1", "2"]},
      {"1", 
       RowBox[{
        SqrtBox["2"], 
        SuperscriptBox["\[Pi]", 
         RowBox[{
          RowBox[{"-", "1"}], "/", "4"}]], " ", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          FractionBox[
           SuperscriptBox["x", "2"], "2"]}]], " ", "x"}], 
       FractionBox["3", "2"]},
      {"2", 
       RowBox[{
        FractionBox["1", 
         SqrtBox["2"]], 
        SuperscriptBox["\[Pi]", 
         RowBox[{
          RowBox[{"-", "1"}], "/", "4"}]], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          FractionBox[
           SuperscriptBox["x", "2"], "2"]}]], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", 
           SuperscriptBox["x", "2"]}], "-", "1"}], ")"}]}], 
       FractionBox["5", "2"]},
      {"3", 
       RowBox[{
        SqrtBox["3"], 
        SuperscriptBox["\[Pi]", 
         RowBox[{
          RowBox[{"-", "1"}], "/", "4"}]], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          FractionBox[
           SuperscriptBox["x", "2"], "2"]}]], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", 
           SuperscriptBox["x", "3"]}], "-", 
          RowBox[{"3", " ", "x"}]}], ")"}]}], 
       FractionBox["7", "2"]}
     },
     GridBoxDividers->{"Columns" -> {
         AbsoluteThickness[2], {False}, 
         AbsoluteThickness[2]}, "ColumnsIndexed" -> {}, "Rows" -> {
         AbsoluteThickness[2], True, {False}, 
         AbsoluteThickness[2]}, "RowsIndexed" -> {}}],
    Background->GrayLevel[0.8],
    GridBoxOptions->{
    GridBoxAlignment->{
     "Columns" -> {{Center}}, "ColumnsIndexed" -> {}, "Rows" -> {{Baseline}}, 
      "RowsIndexed" -> {}}}],
   DisplayForm], TraditionalForm]], "Output",
