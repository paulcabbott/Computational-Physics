           FractionBox["1", "2"], 
           SuperscriptBox[
            RowBox[{
             SuperscriptBox["\[Psi]", "\[Prime]"], "(", "x", ")"}], "2"]}], 
          "+", 
          RowBox[{
           RowBox[{"V", "(", "x", ")"}], " ", 
           SuperscriptBox[
            RowBox[{"\[Psi]", "(", "x", ")"}], "2"]}]}], ")"}], 
        RowBox[{"\[DifferentialD]", "x"}]}]}], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", 
        RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"\[Psi]", "(", "x", ")"}], "2"], 
        RowBox[{"\[DifferentialD]", "x"}]}]}]], "."}]}]}], 
