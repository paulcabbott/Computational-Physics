 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Alpha]", "n"], TraditionalForm]],ExpressionUUID->
  "46fbd4b4-5022-40c0-89bc-b0e3298b0aa5"],
 " are ",
 StyleBox["real",
  FontSlant->"Italic"],
 " parameters.  Since the group generators are matrices, one must compute the \
",
 StyleBox["matrix exponential",
  FontSlant->"Italic"],
 ", ",
 StyleBox["i.e.",
  FontSlant->"Italic"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[ImaginaryI]", " ", "X"}]], "\[Congruent]", 
    RowBox[{"1", "+", 
     RowBox[{"\[ImaginaryI]", " ", "X"}], "-", 
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["X", "2"]}], "-", 
     RowBox[{
      FractionBox["\[ImaginaryI]", "6"], 
      SuperscriptBox["X", "3"]}], "+", 
     RowBox[{
      FractionBox["1", "24"], 
      SuperscriptBox["X", "4"]}], "+", "\[Ellipsis]"}]}], TraditionalForm]],
  ExpressionUUID->"002d3095-d87f-4562-a3ad-7f6f8988ea5f"],
 " using ",
 StyleBox["MatrixExp", "Input"],
