Cell[TextData[{
 
 CounterBox["Section"],
 ".",
 
 CounterBox["Exercise"],
 ". Show that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"\[Mu]", "=", "1"}], "4"], 
     RowBox[{
      SubscriptBox["\[Gamma]", "\[Mu]"], ".", 
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalA]"}], ")"}], ".", 
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalB]"}], ")"}], ".", 
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalC]"}], ")"}], ".", 
      SubscriptBox["\[Gamma]", "\[Mu]"]}]}], "=", 
    RowBox[{
     RowBox[{"-", "2"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalC]"}], ")"}], ".", 
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalB]"}], ")"}], ".", 
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalA]"}], ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"223d8694-8038-4f60-9fe1-97908a32f823"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Tr", "(", 
     RowBox[{
      SubscriptBox["\[Gamma]", "5"], ".", 
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalA]"}], ")"}], ".", 
      RowBox[{"(", 
       RowBox[{"\[Gamma]", ".", "\[ScriptCapitalB]"}], ")"}]}], ")"}], "=", 
    "0"}], TraditionalForm]],ExpressionUUID->
  "a3f15b1f-ec36-41cb-b6b3-be8140fd3fa2"],
 " where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    RowBox[{"(", 
     RowBox[{
      TagBox[
       SubscriptBox["\[Gamma]", "1"],
       HoldForm], ",", 
      TagBox[
       SubscriptBox["\[Gamma]", "2"],
       HoldForm], ",", 
      TagBox[
       SubscriptBox["\[Gamma]", "3"],
       HoldForm], ",", 
      TagBox[
       SubscriptBox["\[Gamma]", "4"],
       HoldForm]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "abebdc63-6ff1-4731-b03b-b355d2f50415"],
 ", ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalA]", TraditionalForm]],ExpressionUUID->
  "7c206530-dcf8-47c8-bacf-7e072b9ac344"],
 ", ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalB]", TraditionalForm]],ExpressionUUID->
  "492ab2a2-eb38-4c60-9e9a-03e5525bd063"],
 ", and ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalC]", TraditionalForm]],ExpressionUUID->
  "cffd5867-c6f1-461a-ab46-1040f3b15c93"],
 " are ",
 StyleBox["arbitrary",
  FontSlant->"Italic"],
 " 4-vectors, ",
 StyleBox["i.e.",
  FontSlant->"Italic"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalA]", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["a", "1"], ",", 
      SubscriptBox["a", "2"], ",", 
      SubscriptBox["a", "3"], ",", 
      SubscriptBox["a", "4"]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "1d11b5ca-d90f-4512-ac77-22eb1ec9a111"],
 ", ",
 StyleBox["etc",
  FontSlant->"Italic"],
 "., and ",
 Cell[BoxData[
  FormBox["Tr", TraditionalForm]],ExpressionUUID->
  "60d0d6a2-31ec-41a6-9ed6-634079bf1589"],
 " denotes the trace of a square matrix (",
 StyleBox["i.e.",
  FontSlant->"Italic"],
 ", the sum of the diagonal entries in the matrix). Note that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Gamma]", ".", "\[ScriptCapitalA]"}], "\[LongEqual]", 
    RowBox[{
     RowBox[{
      SubscriptBox["a", "1"], 
      SubscriptBox["\[Gamma]", "1"]}], "+", 
     RowBox[{
      SubscriptBox["a", "2"], 
      SubscriptBox["\[Gamma]", "2"]}], "+", 
     RowBox[{
      SubscriptBox["a", "3"], 
      SubscriptBox["\[Gamma]", "3"]}], "+", 
     RowBox[{
      SubscriptBox["a", "4"], 
      SubscriptBox["\[Gamma]", "4"]}]}]}], TraditionalForm]],ExpressionUUID->
  "c1ae5e4a-29a2-4b97-8b51-e8b8a1522630"],
 ", etc. "
}], "CodeText",
