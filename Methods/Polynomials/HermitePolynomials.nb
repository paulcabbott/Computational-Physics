Cell[TextData[{
 "Assuming that the interchange of integration and summation is justified, we \
can use the generating function to show that the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "n"], "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "68ec9500-5ecd-4f36-a9e2-f98d6624253e"],
 " are orhogonal as follows:"
}], "Text",
 CellChangeTimes->{{3.419123910491919*^9, 3.4191239278188057`*^9}, {
  3.5157206322426243`*^9, 
  3.515720659671915*^9}},ExpressionUUID->"cb725aef-de38-4abc-bb34-\
c6bb2039a847"],

Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Integral]", 
        RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["s", "2"]}], "+", 
          RowBox[{"2", " ", "s", " ", "x"}]}]], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["t", "2"]}], "+", 
          RowBox[{"2", " ", "t", " ", "x"}]}]], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          SuperscriptBox["x", "2"]}]], 
        RowBox[{"\[DifferentialD]", "x"}]}]}], "\[LongEqual]", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"m", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"n", "=", "0"}], "\[Infinity]"], 
        RowBox[{
         FractionBox[
          SuperscriptBox["s", "m"], 
          RowBox[{"m", "!"}]], 
         FractionBox[
          SuperscriptBox["t", "n"], 
          RowBox[{"n", "!"}]], " ", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
          RowBox[{
           RowBox[{
            SubscriptBox["H", "m"], "(", "x", ")"}], 
           RowBox[{
            SubscriptBox["H", "n"], "(", "x", ")"}], 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"-", 
             SuperscriptBox["x", "2"]}]], 
           RowBox[{"\[DifferentialD]", "x"}]}]}]}]}]}]}], 
     RowBox[{"using", " ", 
      RowBox[{"g", "(", 
       RowBox[{"s", ",", "x"}], ")"}]}]},
    {
     RowBox[{"\[LongEqual]", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"2", "s", " ", "t"}]], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", 
         RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
        RowBox[{
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"x", "-", 
              RowBox[{"(", 
               RowBox[{"s", "+", "t"}], ")"}]}], ")"}], "2"]}]], 
         RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], 
     RowBox[{"completing", " ", "the", " ", "square"}]},
    {
     RowBox[{"\[LongEqual]", 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"2", "s", " ", "t"}]], 
       SqrtBox["\[Pi]"]}]}], 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Integral]", 
        RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          SuperscriptBox["x", "2"]}]], 
        RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
      SqrtBox["\[Pi]"]}]},
    {
     RowBox[{"\[LongEqual]", 
      RowBox[{
       SqrtBox["\[Pi]"], 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"k", "=", "0"}], "\[Infinity]"], 
        FractionBox[
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "s", " ", "t"}], ")"}], "k"], 
         RowBox[{"k", "!"}]]}]}]}], 
     RowBox[{"expanding", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"2", "s", " ", "t"}]]}]}
   }], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.419744855224741*^9, 3.419744872950705*^9}},
