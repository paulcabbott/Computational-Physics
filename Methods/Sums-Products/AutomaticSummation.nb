Cell["the rational function is", "Text",
 CellID->1546377409,ExpressionUUID->"37c97d84-d42d-4267-ae9d-124539a12ff8"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   FormBox[
    FractionBox[
     RowBox[{
      RowBox[{"Take", "[", 
       RowBox[{"%", ",", 
        RowBox[{"p", "+", "1"}]}], "]"}], ".", 
      RowBox[{"Table", "[", 
       RowBox[{
        SuperscriptBox["n", "i"], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0", ",", "p"}], "}"}]}], "]"}]}], 
     RowBox[{
      RowBox[{
       RowBox[{"Take", "[", 
        RowBox[{"%", ",", 
         RowBox[{"-", "q"}]}], "]"}], ".", 
       RowBox[{"Table", "[", 
        RowBox[{
         SuperscriptBox["n", "i"], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "q"}], "}"}]}], "]"}]}], "+", "1"}]],
    TraditionalForm], "//", "Factor"}], TraditionalForm]], "Input",
 CellChangeTimes->{{3.857559437821088*^9, 3.857559437821788*^9}},
 CellID->1118017069,ExpressionUUID->"45f57e84-c0ad-42df-b9e9-1f30edf60e3f"],

Cell[BoxData[
 FormBox[
  RowBox[{"-", 
   FractionBox[
    RowBox[{"\[Alpha]", "+", "n", "-", "1"}], 
    RowBox[{"\[Alpha]", "+", "n"}]]}], TraditionalForm]], "Output",
 CellChangeTimes->{3.857559438617014*^9, 3.8575596884615088`*^9, 
  3.857559831627483*^9},
 CellID->30967433,ExpressionUUID->"ec189d47-0940-416a-a156-293118ccd11b"]
}, Open  ]],

Cell["and the general term of the series reads:", "Text",
 CellID->778603857,ExpressionUUID->"2d8c1f7c-93aa-497c-9f8e-9969264350d3"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"\[ScriptC]", "=", 
   RowBox[{
    RowBox[{"%", "/.", 
     RowBox[{
      RowBox[{"b_.", "+", 
       RowBox[{"n", " ", "a_Integer"}]}], "\[Rule]", "a"}]}], "/.", 
    RowBox[{
     RowBox[{"n", "+", "b_."}], "\[Rule]", "1"}]}]}], 
  TraditionalForm]], "Input",
 CellID->1377163428,ExpressionUUID->"8c1ecb9a-47a0-44b1-bf9c-bb8406d06e02"],

Cell[BoxData[
 FormBox[
  RowBox[{"-", "1"}], TraditionalForm]], "Output",
 CellChangeTimes->{3.857559441696061*^9, 3.8575596885063257`*^9, 
  3.857559831671219*^9},
 CellID->406987030,ExpressionUUID->"c536ef7d-eddc-4166-a405-5d38ad79145c"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"Simplify", "[", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      FractionBox["%%", "\[ScriptC]"], "/.", "\[VeryThinSpace]", 
      RowBox[{
       RowBox[{
        RowBox[{"a_.", " ", "n"}], "+", "b_."}], "\[Rule]", 
       RowBox[{"a", " ", 
        TemplateBox[{
          RowBox[{
            FractionBox["b", "a"], "+", "1"}], "n"},
         "Pochhammer"]}]}]}], ")"}], 
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"\[ScriptC]", " ", "x"}], ")"}], "n"], 
     RowBox[{"n", "!"}]]}], "]"}], TraditionalForm]], "Input",
 CellChangeTimes->{{3.857559449794676*^9, 3.857559462411254*^9}},
 CellID->495597515,ExpressionUUID->"24549114-2928-434c-abf8-c5bd819720d1"],

Cell[BoxData[
 FormBox[
  FractionBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"-", "x"}], ")"}], "n"], " ", 
    TemplateBox[{"\[Alpha]", "n"},
     "Pochhammer"]}], 
   RowBox[{
    RowBox[{"n", "!"}], " ", 
    TemplateBox[{
      RowBox[{"\[Alpha]", "+", "1"}], "n"},
     "Pochhammer"]}]], TraditionalForm]], "Output",
 CellChangeTimes->{3.857559463102634*^9, 3.857559688550486*^9, 
  3.857559831718863*^9},
 CellID->1565851539,ExpressionUUID->"217e6a4f-fbe1-419c-b806-bc40798276fa"]
}, Open  ]],

Cell["which leads to the hypergeometric function", "Text",
 CellID->2002205635,ExpressionUUID->"d35f3353-ce4e-49a1-8457-8b7361f36dd5"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  TagBox[
   RowBox[{"\[ThinSpace]", 
    RowBox[{
     SubscriptBox["", "1"], 
     RowBox[{
      SubscriptBox["F", "1"], "(", 
      RowBox[{
       TagBox[
        TagBox[
         TagBox["\[Alpha]",
          Editable -> True],
         InterpretTemplate[{
           SlotSequence[1]}& ]],
        Editable -> False], ";", 
       TagBox[
        TagBox[
         TagBox[
          RowBox[{"\[Alpha]", "+", "1"}],
          Editable -> True],
         InterpretTemplate[{
           SlotSequence[1]}& ]],
        Editable -> False], ";", 
       TagBox[
        RowBox[{"-", "x"}],
        Editable -> True]}], ")"}]}]}],
   InterpretTemplate[HypergeometricPFQ[#, #2, #3]& ]], 
  TraditionalForm]], "Input",
 CellID->1358955,ExpressionUUID->"72240697-a69a-4ad9-bcd1-de924de0b3ad"],

Cell[BoxData[
 FormBox[
  RowBox[{"\[Alpha]", " ", 
   SuperscriptBox["x", 
    RowBox[{"-", "\[Alpha]"}]], " ", 
   TemplateBox[{"\[Alpha]", "0", "x"},
    "Gamma3"]}], TraditionalForm]], "Output",
 CellChangeTimes->{3.857559465621091*^9, 3.857559688576722*^9, 
  3.8575598317398643`*^9},
 CellID->1365120215,ExpressionUUID->"f35cf1b6-fac7-40c2-9bcd-b17e47706dbe"]
}, Open  ]],

Cell["Alternatively, the infinite series yields", "Text",
 CellID->2007652604,ExpressionUUID->"f26840a4-3360-4944-aabf-ded8cd1c713d"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"n", "=", "0"}], "\[Infinity]"], 
   FormBox[
    FractionBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "x"}], ")"}], "n"], " ", 
      TagBox[
       SubscriptBox[
        RowBox[{"(", "\[Alpha]", ")"}], "n"],
       Pochhammer]}], 
     RowBox[{
      RowBox[{"n", "!"}], " ", 
      TagBox[
       SubscriptBox[
        RowBox[{"(", 
         RowBox[{"\[Alpha]", "+", "1"}], ")"}], "n"],
       Pochhammer]}]],
    TraditionalForm]}], TraditionalForm]], "Input",
 CellID->1354253764,ExpressionUUID->"b1838d65-9030-4af1-867d-0396bb168bc3"],

Cell[BoxData[
 FormBox[
  RowBox[{"\[Alpha]", " ", 
   SuperscriptBox["x", 
    RowBox[{"-", "\[Alpha]"}]], " ", 
   TemplateBox[{"\[Alpha]", "0", "x"},
    "Gamma3"]}], TraditionalForm]], "Output",
 CellChangeTimes->{3.857559470565379*^9, 3.8575596886436033`*^9, 
  3.857559831883507*^9},
 CellID->1911320642,ExpressionUUID->"e5c45fcf-330b-430e-a9aa-e28df19f53c3"]
}, Open  ]],

