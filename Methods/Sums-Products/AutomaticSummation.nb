Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"\[ScriptR]", "=", 
   RowBox[{"ratios", "[", 
    RowBox[{"%", ",", "x"}], "]"}]}], TraditionalForm]], "Input",
