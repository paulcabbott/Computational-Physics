Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  FractionBox["%%", "\[ScriptC]"], TraditionalForm]], "Input",
 CellID->199334176,ExpressionUUID->"40f48257-e1d2-449d-8db1-59f0869d107e"],

Cell[BoxData[
 FormBox["1", TraditionalForm]], "Output",
 CellChangeTimes->{3.857559306086*^9, 3.857559406814129*^9, 
  3.857559688102359*^9, 3.857559831079664*^9},
 CellID->135006712,ExpressionUUID->"3de60298-d337-4355-b276-321e28f13f14"]
}, Open  ]],

Cell["\<\
reveals that the general term of the (hypergeometric) series is\
\>", "Text",
 CellID->1315077352,ExpressionUUID->"116e87a7-3db2-49e0-97d5-bd60e2896149"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"Simplify", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"%", "/.", "\[VeryThinSpace]", 
        RowBox[{
         RowBox[{
          RowBox[{"n", " ", "a_."}], "+", "b_."}], "\[Rule]", 
         RowBox[{"a", " ", 
          FormBox[
           TemplateBox[{
             RowBox[{
               FractionBox["b", "a"], "+", "1"}], "m"},
            "Pochhammer"],
           TraditionalForm]}]}]}], ")"}], " ", 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"\[ScriptC]", " ", "x"}], ")"}], "m"], 
       RowBox[{"m", "!"}]]}], "/.", "\[VeryThinSpace]", 
     RowBox[{"m", "\[Rule]", "n"}]}], "]"}], "//", "Factor"}], 
  TraditionalForm]], "Input",
 CellChangeTimes->{{3.857559312807468*^9, 3.857559324791978*^9}},
 CellID->603360140,ExpressionUUID->"35876d00-caff-4623-842d-4ee5e788443e"],

Cell[BoxData[
 FormBox[
  FractionBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"-", "x"}], ")"}], "n"], 
   RowBox[{"n", "!"}]], TraditionalForm]], "Output",
 CellChangeTimes->{3.8575593280689774`*^9, 3.857559408517941*^9, 
  3.8575596881528482`*^9, 3.857559831154076*^9},
 CellID->256091846,ExpressionUUID->"7780d07e-a98f-4044-9ce3-4a2e4efeba71"]
}, Open  ]],

Cell["The (infinite) series reduces to:", "Text",
 CellID->578427134,ExpressionUUID->"fab90a7c-3499-4b5d-a4df-23ffbb10d396"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"n", "=", "0"}], "\[Infinity]"], 
   FormBox["%",
    TraditionalForm]}], TraditionalForm]], "Input",
 CellID->2113854816,ExpressionUUID->"7725e58c-3e6c-46f6-8b9c-0610a597c3d9"],

Cell[BoxData[
 FormBox[
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{"-", "x"}]], TraditionalForm]], "Output",
 CellChangeTimes->{3.85755933530301*^9, 3.857559412808909*^9, 
  3.857559688222438*^9, 3.8575598311996307`*^9},
 CellID->1735920191,ExpressionUUID->"17d119fb-0605-4d5f-8f73-9a6675357652"]
}, Open  ]],

Cell["\<\
Alternatively, we can compute the sum directly as a hypergeometric function:\
\>", "Text",
 CellID->2045305642,ExpressionUUID->"1fca5df0-90b1-42d2-9ab0-bbe1006f6bcd"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  TagBox[
   RowBox[{"\[ThinSpace]", 
    RowBox[{
     SubscriptBox["", "0"], 
     SubscriptBox["F", "0"], 
     RowBox[{"(", 
      TagBox[
       InterpretationBox["\[InvisibleSpace]",
        {}],
       Editable -> False], 
      RowBox[{";", 
       TagBox[
        InterpretationBox["\[InvisibleSpace]",
         {}],
        Editable -> False], ";", 
       TagBox[
        RowBox[{"-", "x"}],
        Editable -> True]}], ")"}]}]}],
   InterpretTemplate[HypergeometricPFQ[#, #2, #3]& ]], 
  TraditionalForm]], "Input",
 CellID->681187565,ExpressionUUID->"be361511-9718-43cb-8a24-0609019e4262"],

Cell[BoxData[
 FormBox[
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{"-", "x"}]], TraditionalForm]], "Output",
 CellChangeTimes->{3.8575593418731318`*^9, 3.8575594150919523`*^9, 
  3.8575596882619553`*^9, 3.857559831249879*^9},
 CellID->484082798,ExpressionUUID->"5abf448b-78c2-436b-b2f6-e589b46e4efa"]
}, Open  ]],

Cell["As a check, we can expand this into a Maclaurin series:", "Text",
 CellID->1311531150,ExpressionUUID->"5a4053de-3a9a-4f63-a133-3d42998e6e41"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"%", "-", " ", "\[ScriptS]", " ", "+", " ", 
   FormBox[
    SuperscriptBox[
     RowBox[{"O", "(", "x", ")"}], "6"],
    TraditionalForm]}], TraditionalForm]], "Input",
 CellID->1131885242,ExpressionUUID->"045f46c6-d772-4f88-87c2-e05e0f13bc64"],

Cell[BoxData[
 FormBox[
  InterpretationBox[
   RowBox[{"O", "(", 
    SuperscriptBox["x", "6"], ")"}],
   SeriesData[$CellContext`x, 0, {}, 6, 6, 1],
   Editable->False], TraditionalForm]], "Output",
 CellChangeTimes->{3.8575593439918747`*^9, 3.857559416840788*^9, 
  3.8575596882839727`*^9, 3.857559831280303*^9},
 CellID->1185117168,ExpressionUUID->"41d45ded-69e8-4460-8407-0c54ab6f8074"]
}, Open  ]],

Cell["Note that this method is not restricted to numerical series:", "Text",
 CellID->353416651,ExpressionUUID->"8051fa5e-9594-4d51-a62a-44acb0cde5fa"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[ScriptS]", "=", 
    RowBox[{"1", "-", 
     FractionBox[
      RowBox[{"\[Alpha]", " ", "x"}], 
      RowBox[{"\[Alpha]", "+", "1"}]], "+", 
     FractionBox[
      RowBox[{"\[Alpha]", " ", 
       SuperscriptBox["x", "2"]}], 
      RowBox[{
       RowBox[{"2", "!"}], " ", 
       RowBox[{"(", 
        RowBox[{"\[Alpha]", "+", "2"}], ")"}]}]], "-", 
     FractionBox[
      RowBox[{"\[Alpha]", " ", 
       SuperscriptBox["x", "3"]}], 
      RowBox[{
       RowBox[{"3", "!"}], " ", 
       RowBox[{"(", 
        RowBox[{"\[Alpha]", "+", "3"}], ")"}]}]]}]}], ";"}], 
  TraditionalForm]], "Input",
 CellID->2081369402,ExpressionUUID->"df7b1e05-a8a2-4875-85f4-f0dbe62f1354"],

