 CellChangeTimes->{3.857559366014574*^9, 3.857559425596847*^9, 
  3.857559688396003*^9, 3.8575598314480963`*^9},
 CellID->1690310853,ExpressionUUID->"764a4ed1-217c-4110-8684-51bd2cccf192"]
}, Open  ]],

Cell["Solving the linear system of equations:", "Text",
 CellID->2140823855,ExpressionUUID->"8f720533-26c7-47dd-922e-50dacdff14f9"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"LinearSolve", "[", 
    RowBox[{"%", ",", "\[ScriptR]"}], "]"}], "//", "Factor"}], 
  TraditionalForm]], "Input",
 CellChangeTimes->{{3.8575594299942303`*^9, 3.857559429994923*^9}},
 CellID->1885509147,ExpressionUUID->"d199f120-5ed9-4bdd-9504-b433b86b6f80"],

Cell[BoxData[
 FormBox[
  RowBox[{"{", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"\[Alpha]", "-", "1"}], "\[Alpha]"]}], ",", 
    RowBox[{"-", 
     FractionBox["1", "\[Alpha]"]}], ",", 
    FractionBox["1", "\[Alpha]"]}], "}"}], TraditionalForm]], "Output",
 CellChangeTimes->{3.857559432128592*^9, 3.857559688442165*^9, 
