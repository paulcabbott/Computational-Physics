Cell["\<\
To assess the quality of these results, it is straightforward to compute \
accurate energy eigenvalues using standard variational methods. Using \
integration by parts, the radial Schr\[ODoubleDot]dinger equation can be \
rewritten as\
\>", "Text",
 CellID->1545761643,ExpressionUUID->"85cd09f7-1fdb-41ce-a9b6-ac731c50833a"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"-", "2"}], " ", "\[ScriptCapitalE]", " ", 
    FormBox[
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"R", "\[VerticalSeparator]", "R"}], "\[RightAngleBracket]"}],
     TraditionalForm]}], "\[LongEqual]", 
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[PartialD]", "r"], "R"}], "\[VerticalSeparator]", 
      RowBox[{
       SubscriptBox["\[PartialD]", "r"], "R"}]}], "\[RightAngleBracket]"}], 
    "+", 
    RowBox[{"l", " ", 
     RowBox[{"(", 
      RowBox[{"l", "+", "1"}], ")"}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"R", "\[VerticalSeparator]", 
       FractionBox["1", 
        SuperscriptBox["r", "2"]], "\[VerticalSeparator]", "R"}], 
      "\[RightAngleBracket]"}]}], "-", 
    RowBox[{"2", "\[Alpha]", " ", 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"R", "\[VerticalSeparator]", 
       FractionBox[
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "r"}], "/", "b"}]], 
        RowBox[{"1", "-", 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{
           RowBox[{"-", "r"}], "/", "b"}]]}]], "\[VerticalSeparator]", "R"}], 
      "\[RightAngleBracket]"}]}], "+", 
    RowBox[{"2", "\[Beta]", " ", 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"R", "\[VerticalSeparator]", 
       FractionBox[
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "r"}], "/", "b"}]], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "r"}], "/", "b"}]]}], ")"}], "2"]], 
       "\[VerticalSeparator]", "R"}], "\[RightAngleBracket]"}]}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
