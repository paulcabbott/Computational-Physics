Cell[TextData[{
 "which is equivalent equation (3) of Dong ",
 StyleBox["et al.",
  FontSlant->"Italic"],
 " [",
 
 CounterBox["Reference", "Dong et al (2007)"],
 "]. Scaling lengths by ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],ExpressionUUID->
  "d29f3191-c608-4075-bfa3-00183e2c983a"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "\[Rule]", 
    RowBox[{"b", " ", "\[Rho]"}]}], TraditionalForm]],ExpressionUUID->
  "91ea1ffc-411d-41d0-a524-3ec7887d9d7c"],
 "), energies by ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["b", 
    RowBox[{"-", "2"}]], TraditionalForm]],ExpressionUUID->
  "7716958b-3659-4483-97d0-6297fad2e0e5"],
 ", and introducing ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalA]", "=", 
    RowBox[{"2", 
     SuperscriptBox["b", "2"], "\[Alpha]"}]}], TraditionalForm]],
  ExpressionUUID->"b9ab53b7-1316-4b35-9dc0-38254c1882af"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalB]", "=", 
    RowBox[{"2", 
     SuperscriptBox["b", "2"], "\[Beta]"}]}], TraditionalForm]],
  ExpressionUUID->"818f471f-6dcc-4951-864c-70ac2be97151"],
 ", the scaled equation reads"
}], "Text",
 CellID->1996957195,ExpressionUUID->"88231f93-d80b-4a28-b48d-5d3326debb78"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"ScaledEqn", "[", "1", "]"}], "=", 
   RowBox[{"Collect", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox["b", "2"], 
         RowBox[{"Eqn", "[", "3", "]"}]}], "/.", 
