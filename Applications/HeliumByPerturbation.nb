          SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], " ", 
          RowBox[{
           RowBox[{"exp", "(", 
            RowBox[{
             RowBox[{
              RowBox[{"-", 
               SubscriptBox["\[Lambda]", "1"]}], " ", 
              RowBox[{"(", 
               RowBox[{"y", "+", "z"}], ")"}]}], "-", 
             RowBox[{
              SubscriptBox["\[Lambda]", "2"], " ", 
              RowBox[{"(", 
               RowBox[{"x", "+", "z"}], ")"}]}], "-", 
             RowBox[{
              SubscriptBox["\[Lambda]", "12"], " ", 
              RowBox[{"(", 
               RowBox[{"x", "+", "y"}], ")"}]}]}], ")"}], " ", 
           RowBox[{"\[DifferentialD]", "x"}], " ", 
           RowBox[{"\[DifferentialD]", "y"}], 
