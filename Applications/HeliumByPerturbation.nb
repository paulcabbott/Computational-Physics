Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{"\[Psi]", 
     RowBox[{
     "\[LeftBracketingBar]", "\[GothicCapitalO]", "\[RightBracketingBar]"}], 
     "\[Psi]"}], "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{"8", 
     SuperscriptBox["\[Pi]", "2"], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", 
         RowBox[{"\[LeftBracketingBar]", 
          RowBox[{
           SubscriptBox["r", "1"], "-", 
           SubscriptBox["r", "2"]}], "\[RightBracketingBar]"}], 
         RowBox[{
          SubscriptBox["r", "1"], "+", 
          SubscriptBox["r", "2"]}]], 
        RowBox[{
         SuperscriptBox["\[Psi]", "*"], "\[GothicCapitalO]", " ", "\[Psi]", 
         " ", 
         SubscriptBox["r", "1"], 
         SubscriptBox["r", "2"], 
         SubscriptBox["r", "12"], 
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["r", "12"]}], 
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["r", "2"]}], 
         RowBox[{"\[DifferentialD]", 
          SubscriptBox["r", "1"]}]}]}]}]}]}], "\[Congruent]", 
    RowBox[{"8", 
     SuperscriptBox["\[Pi]", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
        RowBox[{
         SubsuperscriptBox["\[Integral]", "0", 
          SubscriptBox["r", "1"]], 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           RowBox[{
            SubscriptBox["r", "1"], "-", 
            SubscriptBox["r", "2"]}], 
           RowBox[{
            SubscriptBox["r", "1"], "+", 
            SubscriptBox["r", "2"]}]], 
          RowBox[{
           SuperscriptBox["\[Psi]", "*"], "\[GothicCapitalO]", " ", "\[Psi]", 
           " ", 
           SubscriptBox["r", "1"], 
           SubscriptBox["r", "2"], 
           SubscriptBox["r", "12"], 
           RowBox[{"\[DifferentialD]", 
            SubscriptBox["r", "12"]}], 
           RowBox[{"\[DifferentialD]", 
            SubscriptBox["r", "2"]}], 
           RowBox[{"\[DifferentialD]", 
            SubscriptBox["r", "1"]}]}]}]}]}], "+", 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
        RowBox[{
         SubsuperscriptBox["\[Integral]", 
          SubscriptBox["r", "1"], "\[Infinity]"], 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           RowBox[{
            SubscriptBox["r", "2"], "-", 
            SubscriptBox["r", "1"]}], 
           RowBox[{
            SubscriptBox["r", "1"], "+", 
            SubscriptBox["r", "2"]}]], 
          RowBox[{
           SuperscriptBox["\[Psi]", "*"], "\[GothicCapitalO]", " ", "\[Psi]", 
           " ", 
           SubscriptBox["r", "1"], 
           SubscriptBox["r", "2"], 
           SubscriptBox["r", "12"], 
           RowBox[{"\[DifferentialD]", 
            SubscriptBox["r", "12"]}], 
           RowBox[{"\[DifferentialD]", 
            SubscriptBox["r", "2"]}], 
           RowBox[{"\[DifferentialD]", 
            SubscriptBox["r", "1"]}]}]}]}]}]}], ")"}]}]}]}], 
