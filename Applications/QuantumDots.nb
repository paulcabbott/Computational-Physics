Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"InitialiseCoefficients", "[", 
    RowBox[{"\[ScriptCapitalS]_", ",", "\[ScriptCapitalN]_", ",", "k_"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Chi]", ",", "m", ",", "n"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Chi]", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "\[ScriptCapitalN]"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"n", "=", 
          FormBox[
           RowBox[{
            RowBox[{
            "\[ScriptCapitalS]", "\[LeftDoubleBracket]", "i", 
             "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "1", 
            "\[RightDoubleBracket]"}],
           TraditionalForm]}], ";", 
         RowBox[{"m", "=", 
          FormBox[
           RowBox[{
            RowBox[{
            "\[ScriptCapitalS]", "\[LeftDoubleBracket]", "i", 
             "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "2", 
            "\[RightDoubleBracket]"}],
           TraditionalForm]}], ";", "\[IndentingNewLine]", 
         RowBox[{"c", "/:", 
          SubscriptBox["c", 
           RowBox[{
            FormBox[
             RowBox[{
             "\[ScriptCapitalS]", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}],
             TraditionalForm], ",", "0"}]], "=", 
          RowBox[{"NormaliseCoefficients", "[", 
           RowBox[{
            RowBox[{"ReplacePart", "[", 
             RowBox[{
              FormBox["\[Chi]",
               TraditionalForm], ",", "1", ",", 
              RowBox[{
               FormBox["n",
                TraditionalForm], "+", "1"}]}], "]"}], ",", "k", ",", "m"}], 
           "]"}]}], ";"}], "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "\[ScriptCapitalS]", "]"}]}], "}"}]}], 
       "]"}]}]}], "\[IndentingNewLine]", "]"}]}], TraditionalForm]], "Input",
 InitializationCell->True,
