Cell[TextData[{
 "To solve the ",
 StyleBox["N",
  FontSlant->"Italic"],
 "-electron Schr\[ODoubleDot]dinger equation we use the Hartree-Fock method",
 " [",
 
 CounterBox["Reference", "Ref: Bransden & Joachain 1983"],
 "]",
 ". The Hartree-Fock approach is a particular case of the variational method, \
in which the trial wavefunction is assumed to be a ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "\[Times]", "N"}], TraditionalForm]],ExpressionUUID->
  "42d55605-7d5c-407d-8da4-d06cee0e0f52"],
 " Slater determinant,"
}], "Text",ExpressionUUID->"8904227b-098a-4dea-97ab-5e5f8e1e1132"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[CapitalPsi]", "(", 
     RowBox[{
      SubscriptBox["q", "1"], ",", 
      SubscriptBox["q", "2"], ",", 
      SubscriptBox["\[Ellipsis]q", "N"]}], ")"}], "=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"N", "!"}]]], " ", 
     RowBox[{"\[LeftBracketingBar]", "\[NoBreak]", GridBox[{
        {
         RowBox[{
          SubscriptBox["\[Psi]", "\[Alpha]"], "(", 
          SubscriptBox["q", "1"], ")"}], 
         RowBox[{
          SubscriptBox["\[Psi]", "\[Beta]"], "(", 
          SubscriptBox["q", "1"], ")"}], "\[CenterEllipsis]", 
         RowBox[{
          SubscriptBox["\[Psi]", "\[Nu]"], "(", 
          SubscriptBox["q", "1"], ")"}]},
        {
         RowBox[{
          SubscriptBox["\[Psi]", "\[Alpha]"], "(", 
          SubscriptBox["q", "2"], ")"}], 
         RowBox[{
          SubscriptBox["\[Psi]", "\[Beta]"], "(", 
          SubscriptBox["q", "2"], ")"}], "\[CenterEllipsis]", 
         RowBox[{
          SubscriptBox["\[Psi]", "\[Nu]"], "(", 
          SubscriptBox["q", "2"], ")"}]},
        {"\[VerticalEllipsis]", "\[VerticalEllipsis]", 
         "\[DescendingEllipsis]", "\[VerticalEllipsis]"},
        {
         RowBox[{
          SubscriptBox["\[Psi]", "\[Alpha]"], "(", 
          SubscriptBox["q", "N"], ")"}], 
         RowBox[{
          SubscriptBox["\[Psi]", "\[Beta]"], "(", 
          SubscriptBox["q", "N"], ")"}], "\[CenterEllipsis]", 
         RowBox[{
          SubscriptBox["\[Psi]", "\[Nu]"], "(", 
          SubscriptBox["q", "N"], ")"}]}
       }], "\[NoBreak]", "\[RightBracketingBar]"}]}]}], ","}], 
  TraditionalForm]], "DisplayFormula",
 CellTags->
  "Equation: Slater \
determinant",ExpressionUUID->"4ca7ce7a-5399-4891-b03e-681ab6772db1"],

Cell[TextData[{
 "where the symbols ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", ",", "\[Beta]", ",", 
    RowBox[{"...", "\[Nu]"}]}], TraditionalForm]],ExpressionUUID->
  "a223981b-fcff-4c35-a040-69b7efc67f55"],
 " correspond to the ",
 StyleBox["N",
  FontSlant->"Italic"],
