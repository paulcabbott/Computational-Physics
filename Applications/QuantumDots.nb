       RowBox[{"\[Eta]", "=", "0"}], ",", 
       RowBox[{"\[Tau]1", "=", "0"}], ",", 
       RowBox[{"\[Tau]2", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Tau]1", "=", 
       RowBox[{"First", "[", 
        RowBox[{"Timing", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"InitialiseMatrixElements", "[", "\[ScriptCapitalN]", "]"}],
