Cell[TextData[{
 StyleBox["StateSolve",
  FontWeight->"Bold"],
 " solves the Rayleigh-Ritz matrix equation, Eq. (",
 
 CounterBox["DisplayFormulaNumbered", 
  "Equation: Generalised matrix eigenvalue problem"],
 "), for each electron in the system, and then assigns a new set of \
normalised coefficients based on the solutions."
}], "Text",ExpressionUUID->"b4d55b42-7531-4487-a92f-375ae2d13324"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"StateSolve", "[", 
    RowBox[{
    "\[ScriptCapitalS]_", ",", "\[ScriptCapitalN]_", ",", "k_", ",", "V0_", 
     ",", "\[Eta]_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Lambda]", ",", "\[CurlyEpsilon]", ",", "\[Chi]"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"\[Lambda]", "=", 
         FormBox[
          RowBox[{
          "\[ScriptCapitalS]", "\[LeftDoubleBracket]", "i", 
           "\[RightDoubleBracket]"}],
          TraditionalForm]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[CurlyEpsilon]", ",", "\[Chi]"}], "}"}], "=", 
         RowBox[{"Transpose", "[", 
          RowBox[{"Select", "[", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"Chop", "[", 
               RowBox[{"Eigensystem", "[", 
                RowBox[{
                 RowBox[{
                  SuperscriptBox[
                   RowBox[{"(", 
                    RowBox[{
                    SubscriptBox["\[ScriptCapitalB]", "\[ScriptCapitalN]"], 
                    "[", 
                    RowBox[{"k", ",", 
                    RowBox[{
                    "\[Lambda]", "\[LeftDoubleBracket]", "2", 
                    "\[RightDoubleBracket]"}]}], "]"}], ")"}], 
                   TagBox[
                    RowBox[{"-", "1"}],
                    Inverse]], ".", 
                  RowBox[{
                   SubscriptBox["\[ScriptCapitalA]", "\[ScriptCapitalN]"], 
                   "[", 
                   RowBox[{
                   "\[Lambda]", ",", "\[ScriptCapitalS]", ",", "k", ",", "V0",
                     ",", "\[Eta]"}], "]"}]}], "//", "N"}], "]"}], "]"}], 
              ")"}], "T"], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"First", "[", "#1", "]"}], "<", "0"}], "&"}]}], "]"}], 
          "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"c", "/:", 
         SubscriptBox["c", 
          RowBox[{"\[Lambda]", ",", 
           RowBox[{"\[Eta]", "+", "1"}]}]], "=", 
         RowBox[{"Part", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"NormaliseCoefficients", "[", 
              RowBox[{"#1", ",", "k", ",", 
               RowBox[{
               "\[Lambda]", "\[LeftDoubleBracket]", "2", 
                "\[RightDoubleBracket]"}]}], "]"}], "&"}], "/@", "\[Chi]"}], 
           ",", 
           RowBox[{
            RowBox[{
            "\[Lambda]", "\[LeftDoubleBracket]", "1", 
             "\[RightDoubleBracket]"}], "+", "1"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"\[ScriptCapitalE]", "/:", 
         SubscriptBox["\[ScriptCapitalE]", 
          RowBox[{"\[Lambda]", ",", 
           RowBox[{"\[Eta]", "+", "1"}]}]], "=", 
         FormBox[
          RowBox[{"\[CurlyEpsilon]", "\[LeftDoubleBracket]", 
           RowBox[{
            RowBox[{
            "\[Lambda]", "\[LeftDoubleBracket]", "1", 
             "\[RightDoubleBracket]"}], "+", "1"}], "\[RightDoubleBracket]"}],
          
          TraditionalForm]}], ";"}], "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "[", "\[ScriptCapitalS]", "]"}]}], "}"}]}], 
      "]"}]}], "\[IndentingNewLine]", "]"}]}], TraditionalForm]], "Input",
 InitializationCell->True,
 CellLabel->"In[27]:=",ExpressionUUID->"d22ba3e0-7111-4687-b7da-98b3a6b302aa"],

Cell[TextData[{
 "Upon exiting the self-consistency loop, we save the values for the \
\[GothicD] and \[GothicF] integrals using the ",
 StyleBox["SaveIntegrals",
  FontWeight->"Bold"],
 " module (they are saved to the files ",
 StyleBox["d.mx",
  FontSlant->"Italic"],
 " and ",
 StyleBox["f.mx",
  FontSlant->"Italic"],
 " in the ",
 StyleBox["HFData",
  FontSlant->"Italic"],
 " subdirectory of the home directory). This is done to increase efficiency \
in further calculations of different electronic configurations, since it is \
not necessary to re\[Hyphen]evaluate these time consuming integrals."
}], "Text",ExpressionUUID->"35316d51-0b6f-4a62-8265-b9397d4d9a43"],

