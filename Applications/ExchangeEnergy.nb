              "EllipticK"], "\[Rule]", 
             RowBox[{"\[ScriptK]", " ", 
              RowBox[{"(", 
               RowBox[{"r", "+", "s"}], ")"}]}]}], ",", 
            RowBox[{
             TemplateBox[{
               FractionBox[
                RowBox[{"4", " ", "r", " ", "s"}], 
                SuperscriptBox[
                 RowBox[{"(", 
                   RowBox[{"r", "+", "s"}], ")"}], "2"]]},
              "EllipticE"], "\[Rule]", 
             FractionBox["\[ScriptE]", 
              RowBox[{"r", "+", "s"}]]}]}], "}"}]}], "//", "Factor"}], "//", 
        "Expand"}], ")"}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
