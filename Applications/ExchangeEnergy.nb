  RowBox[{
   RowBox[{
    SubscriptBox["\[ScriptCapitalR]", 
     RowBox[{"n_", ",", "l_", ",", 
      RowBox[{"Z_", ":", "1"}]}]], "(", "\[Rho]_", ")"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{"2", "Z"}], 
     RowBox[{"n", "-", 
      RowBox[{"1", "/", "2"}]}]], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "-", 
           TemplateBox[{"l"},
            "Abs"], "-", "1"}], ")"}], "!"}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "+", 
           TemplateBox[{"l"},
            "Abs"], "-", "1"}], ")"}], "!"}]], 
       FractionBox["1", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "n"}], "-", "1"}], ")"}]]}], ")"}], 
     RowBox[{"1", "/", "2"}]], " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "Z"}], " ", 
      RowBox[{"\[Rho]", "/", 
       RowBox[{"(", 
        RowBox[{"n", "-", 
         RowBox[{"1", "/", "2"}]}], ")"}]}]}]], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"2", "Z", " ", "\[Rho]"}], 
       RowBox[{"n", "-", 
        RowBox[{"1", "/", "2"}]}]], ")"}], 
     TemplateBox[{"l"},
      "Abs"]], 
    RowBox[{
     SubsuperscriptBox["L", 
      RowBox[{"n", "-", "1", "-", 
       FormBox[
        TemplateBox[{"l"},
         "Abs"],
        TraditionalForm]}], 
      RowBox[{"2", 
       RowBox[{"\[LeftBracketingBar]", "l", "\[RightBracketingBar]"}]}]], "(", 
     FractionBox[
      RowBox[{"2", "Z", " ", "\[Rho]"}], 
      RowBox[{"n", "-", 
       RowBox[{"1", "/", "2"}]}]], ")"}]}]}], TraditionalForm]], "Input",
