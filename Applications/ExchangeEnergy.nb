  3.460855053418091*^9}},ExpressionUUID->"61097743-fefc-4951-a11d-\
eab76f9762c2"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[CapitalDelta]", "(", "\[Psi]_", ")"}], ":=", 
   RowBox[{
    FormBox[
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], "\[Psi]"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["r", "2"]}]], "+", 
      RowBox[{
       FractionBox["1", "r"], 
       FractionBox[
        RowBox[{"\[PartialD]", "\[Psi]"}], 
        RowBox[{"\[PartialD]", "r"}]]}]}],
     TraditionalForm], "+", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["r", "2"]], 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[PartialD]", "2"], "\[Psi]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["\[Phi]", "2"]}]]}]}]}], TraditionalForm]], "Input",
 CellChangeTimes->{{3.460848014908617*^9, 3.4608480911418858`*^9}, {
  3.460852967200083*^9, 3.460852968580454*^9}, {3.460855031648933*^9, 
  3.46085509174122*^9}},ExpressionUUID->"573955d9-bb7d-45b4-8d6d-\
1107a809c3af"]
}, Open  ]],

