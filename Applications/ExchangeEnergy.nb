Cell[TextData[{
 "With a closed-form, one immediately can compute the asymptotic behaviour. \
For example, check the value of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Epsilon]", "x"], "(", "0", ")"}], TraditionalForm]],
  ExpressionUUID->"c370eaae-a7d4-4625-b84c-1d1248af97b9"],
 " by series expansion about ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "f2834fae-c358-4991-a8e3-7a15f79accf8"],
 "."
}], "Text",
 CellChangeTimes->{{3.4613027382517014`*^9, 3.4613027804329367`*^9}, {
  3.4613057386111517`*^9, 3.461305742455048*^9}, {3.46130586269355*^9, 
  3.461305865194531*^9}},ExpressionUUID->"abecf9e4-b376-4d2e-943e-\
43609abeb3fb"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Epsilon]", "(", "r", ")"}], "+", 
   SuperscriptBox[
    RowBox[{"O", "[", "r", "]"}], "3"]}], TraditionalForm]], "Input",
 CellChangeTimes->{{3.461302754632571*^9, 
  3.461302768292995*^9}},ExpressionUUID->"fcc8fe95-f9c5-4fbc-bd99-\
448b5a01f0ee"],

Cell[BoxData[
 FormBox[
  InterpretationBox[
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"515", " ", "\[Pi]"}], "1944"]}], "+", 
    FractionBox[
     RowBox[{"515", " ", "\[Pi]", " ", "r"}], "972"], "+", 
    FractionBox[
     RowBox[{
      SuperscriptBox["r", "2"], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"784", " ", "\[Pi]", " ", 
          RowBox[{"log", "(", "r", ")"}]}]}], "-", 
        RowBox[{"784", " ", 
         TagBox["\[DoubledGamma]",
          Function[{}, EulerGamma]], " ", "\[Pi]"}], "-", 
        RowBox[{"1573", " ", "\[Pi]"}], "+", 
        RowBox[{"\[Pi]", " ", 
         RowBox[{"log", "(", "6", ")"}]}], "+", 
        RowBox[{"54", " ", "\[Pi]", " ", 
         RowBox[{"log", "(", "3", ")"}]}], "+", 
        RowBox[{"729", " ", "\[Pi]", " ", 
         RowBox[{"log", "(", "2", ")"}]}]}], ")"}]}], "2916"], "+", 
    InterpretationBox[
     RowBox[{"O", "(", 
      SuperscriptBox["r", "3"], ")"}],
     SeriesData[$CellContext`r, 0, {}, 0, 3, 1],
     Editable->False]}],
   SeriesData[$CellContext`r, 0, {
    Rational[-515, 1944] Pi, Rational[515, 972] Pi, 
     Rational[1, 2916] ((-1573) Pi - (784 EulerGamma) 
      Pi + (729 Pi) Log[2] + (54 Pi) Log[3] + Pi Log[6] - (784 Pi) 
      Log[$CellContext`r])}, 0, 3, 1],
   Editable->False], TraditionalForm]], "Output",
 CellChangeTimes->{{3.4613027619808493`*^9, 3.46130278182762*^9}, 
   3.461305447491901*^9, 3.46130704769136*^9, 
   3.461456249559586*^9},ExpressionUUID->"486e008e-8b7a-4a73-9548-\
8fdf7f5a8613"]
}, Open  ]]
