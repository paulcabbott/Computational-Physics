Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"Series", "[", 
    RowBox[{
     RowBox[{"\[CapitalGamma]", "(", 
      RowBox[{"n", "+", "1"}], ")"}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "\[Infinity]", ",", "3"}], "}"}]}], "]"}], "//", 
   "Simplify"}], TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", "n"}]], " ", 
   SuperscriptBox["n", "n"], " ", 
   RowBox[{"(", 
    InterpretationBox[
     RowBox[{
      FractionBox[
       SqrtBox[
        RowBox[{"2", " ", "\[Pi]"}]], 
       SqrtBox[
        FractionBox["1", "n"]]], "+", 
      RowBox[{
       FractionBox["1", "6"], " ", 
       SqrtBox[
        FractionBox["\[Pi]", "2"]], " ", 
       SqrtBox[
        FractionBox["1", "n"]]}], "+", 
      RowBox[{
       FractionBox["1", "144"], " ", 
       SqrtBox[
        FractionBox["\[Pi]", "2"]], " ", 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox["1", "n"], ")"}], 
        RowBox[{"3", "/", "2"}]]}], "+", 
      InterpretationBox[
       RowBox[{"O", "(", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["1", "n"], ")"}], 
         RowBox[{"5", "/", "2"}]], ")"}],
       SeriesData[n, 
        DirectedInfinity[1], {}, -1, 5, 2],
       Editable->False]}],
     SeriesData[n, 
      DirectedInfinity[
      1], {(2 Pi)^Rational[1, 2], 0, 
       Rational[1, 6] (Rational[1, 2] Pi)^Rational[1, 2], 0, 
       Rational[1, 144] (Rational[1, 2] Pi)^Rational[1, 2]}, -1, 5, 2],
     Editable->False], ")"}]}], TraditionalForm]], "Output"]
