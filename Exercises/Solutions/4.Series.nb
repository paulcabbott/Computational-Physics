Cell["\<\
Now determine where the integrand takes on its maximum value,\
\>", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[PartialD]", "t"], 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox[
          SuperscriptBox["t", "3"], "3"]}], "+", 
        RowBox[{"x", " ", "t"}]}], ")"}]}], "\[Equal]", "0"}], ",", "t"}], 
   "]"}], TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  RowBox[{"{", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"t", "\[Rule]", 
      RowBox[{"-", 
       SqrtBox["x"]}]}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"t", "\[Rule]", 
      SqrtBox["x"]}], "}"}]}], "}"}], TraditionalForm]], "Output"]
}, Open  ]],

Cell["and compute the integrand there.", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"exp", "(", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       SuperscriptBox["t", "3"], "3"]}], "+", 
     RowBox[{"x", " ", "t"}]}], ")"}], "/.", 
   RowBox[{"Last", "[", "%", "]"}]}], TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  SuperscriptBox["\[ExponentialE]", 
   FractionBox[
    RowBox[{"2", " ", 
     SuperscriptBox["x", 
      RowBox[{"3", "/", "2"}]]}], "3"]], TraditionalForm]], "Output"]
}, Open  ]],

Cell["Next we expand the integrand around the (sharp) maximum", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"exp", "(", 
   RowBox[{"Normal", "[", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       SuperscriptBox["t", "3"], "3"]}], "+", 
     RowBox[{"x", " ", "t"}], "+", 
     SuperscriptBox[
      RowBox[{"O", "[", 
       RowBox[{"t", ",", 
        SqrtBox["x"]}], "]"}], "3"]}], "]"}], ")"}], 
  TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox["x", 
       RowBox[{"3", "/", "2"}]]}], "3"], "-", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SqrtBox["x"]}], ")"}], "2"], " ", 
     SqrtBox["x"]}]}]], TraditionalForm]], "Output"]
}, Open  ]],

Cell["\<\
So to compute the asymptotic expansion, we need to compute the following \
integral\
\>", "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"Hi", "(", "x", ")"}], "~", 
    FractionBox["1", "\[Pi]"]}], 
   SuperscriptBox["\[ExponentialE]", 
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox["x", 
       RowBox[{"3", "/", "2"}]]}], "3"]], 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"t", "-", 
           SqrtBox["x"]}], ")"}], "2"]}], " ", 
       SqrtBox["x"]}]], 
     RowBox[{
      RowBox[{"\[DifferentialD]", "t"}], "."}]}]}]}], 
  TraditionalForm]], "DisplayFormula"],

Cell[TextData[{
 "Although ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " can compute this integral exactly,"
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"Assuming", "[", 
   RowBox[{
    RowBox[{"x", ">", "0"}], ",", 
    RowBox[{
     FractionBox["1", "\[Pi]"], 
     SuperscriptBox["\[ExponentialE]", 
      FractionBox[
       RowBox[{"2", " ", 
        SuperscriptBox["x", 
         RowBox[{"3", "/", "2"}]]}], "3"]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"t", "-", 
             SqrtBox["x"]}], ")"}], "2"]}], " ", 
         SqrtBox["x"]}]], 
       RowBox[{"\[DifferentialD]", "t"}]}]}]}]}], "]"}], 
  TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  FractionBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     FractionBox[
      RowBox[{"2", " ", 
       SuperscriptBox["x", 
        RowBox[{"3", "/", "2"}]]}], "3"]], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"erf", "(", 
       SuperscriptBox["x", 
        RowBox[{"3", "/", "4"}]], ")"}], "+", "1"}], ")"}]}], 
   RowBox[{"2", " ", 
    SqrtBox["\[Pi]"], " ", 
    RadicalBox["x", "4"]}]], TraditionalForm]], "Output"]
}, Open  ]],

Cell[TextData[{
 "we only need an approximate value for large ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"erf", "(", "x", ")"}], "\[Rule]", "1"}], TraditionalForm]]],
 ","
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"%", "/.", 
   RowBox[{
    RowBox[{"erf", "(", "_", ")"}], "\[Rule]", "1"}]}], 
  TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  FractionBox[
   SuperscriptBox["\[ExponentialE]", 
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox["x", 
       RowBox[{"3", "/", "2"}]]}], "3"]], 
   RowBox[{
    SqrtBox["\[Pi]"], " ", 
    RadicalBox["x", "4"]}]], TraditionalForm]], "Output"]
}, Open  ]],

Cell["Hence the first term in the asymptotic series is", "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"Hi", "(", "x", ")"}], "~", 
    FractionBox[
     SuperscriptBox["\[ExponentialE]", 
      FractionBox[
       RowBox[{"2", " ", 
        SuperscriptBox["x", 
         RowBox[{"3", "/", "2"}]]}], "3"]], 
     RowBox[{
      SqrtBox["\[Pi]"], " ", 
      RadicalBox["x", "4"]}]]}], "."}], TraditionalForm]], "DisplayFormula"],

Cell[TextData[{
 "Alternatively, to compute the integral approximately, we simply extend the \
range of integration to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", "\[Infinity]"}], TraditionalForm]]],
 " because the integrand is negligible there."
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{"Assuming", "[", 
   RowBox[{
    RowBox[{"x", ">", "0"}], ",", 
    RowBox[{
     FractionBox["1", "\[Pi]"], 
     SuperscriptBox["\[ExponentialE]", 
      FractionBox[
       RowBox[{"2", " ", 
        SuperscriptBox["x", 
         RowBox[{"3", "/", "2"}]]}], "3"]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"t", "-", 
             SqrtBox["x"]}], ")"}], "2"]}], " ", 
         SqrtBox["x"]}]], 
       RowBox[{"\[DifferentialD]", "t"}]}]}]}]}], "]"}], 
  TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  FractionBox[
   SuperscriptBox["\[ExponentialE]", 
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox["x", 
       RowBox[{"3", "/", "2"}]]}], "3"]], 
   RowBox[{
    SqrtBox["\[Pi]"], " ", 
    RadicalBox["x", "4"]}]], TraditionalForm]], "Output"]
}, Open  ]],

Cell["\<\
Computing higher-order terms is straightforward. Expanding the integrand to \
higher order,\
\>", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        SuperscriptBox["t", "3"], "3"]}], "+", 
      RowBox[{"x", " ", "t"}]}], ")"}], 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      FractionBox[
       RowBox[{"2", " ", 
        SuperscriptBox["x", 
         RowBox[{"3", "/", "2"}]]}], "3"], "-", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"t", "-", 
          SqrtBox["x"]}], ")"}], "2"], " ", 
       SqrtBox["x"]}]}]]], "+", 
   SuperscriptBox[
    RowBox[{"O", "[", 
     RowBox[{"t", ",", 
      SqrtBox["x"]}], "]"}], "20"]}], TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  InterpretationBox[
   RowBox[{"1", "-", 
    RowBox[{
     FractionBox["1", "3"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SqrtBox["x"]}], ")"}], "3"]}], "+", 
    RowBox[{
     FractionBox["1", "18"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SqrtBox["x"]}], ")"}], "6"]}], "-", 
    RowBox[{
     FractionBox["1", "162"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SqrtBox["x"]}], ")"}], "9"]}], "+", 
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SqrtBox["x"]}], ")"}], "12"], "1944"], "-", 
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SqrtBox["x"]}], ")"}], "15"], "29160"], "+", 
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"t", "-", 
        SqrtBox["x"]}], ")"}], "18"], "524880"], "+", 
    InterpretationBox[
     RowBox[{"O", "(", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"t", "-", 
         SqrtBox["x"]}], ")"}], "20"], ")"}],
     SeriesData[t, x^Rational[1, 2], {}, 0, 20, 1],
     Editable->False]}],
   SeriesData[t, x^Rational[1, 2], {1, 0, 0, 
     Rational[-1, 3], 0, 0, 
     Rational[1, 18], 0, 0, 
     Rational[-1, 162], 0, 0, 
     Rational[1, 1944], 0, 0, 
