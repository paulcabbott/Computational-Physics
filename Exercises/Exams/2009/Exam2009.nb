Cell[TextData[{
 "Define a function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"b", ",", "c"}], "}"}], ",", "ord"}], ")"}], TraditionalForm]]],
 " that returns ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["x", "+"], ",", 
     SubscriptBox["x", "-"]}], "}"}], TraditionalForm]]],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ord", "=", "1"}], TraditionalForm]]],
 ",   ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["x", "-"], ",", 
     SubscriptBox["x", "+"]}], "}"}], TraditionalForm]]],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ord", "=", 
    RowBox[{"-", "1"}]}], TraditionalForm]]],
 " or a ",
 Cell[BoxData[
  FormBox[
   ButtonBox["RandomChoice",
    BaseStyle->"Link",
    ButtonData->"paclet:ref/RandomChoice"], TraditionalForm]]],
 " of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["x", "\[PlusMinus]"], ",", 
     SubscriptBox["x", "\[MinusPlus]"]}], "}"}], TraditionalForm]]],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ord", "=", "0"}], TraditionalForm]]],
 ".\[LineSeparator]Test it by reproducing the examples given above:  ie on  ",
 Cell[BoxData[
  StyleBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{"f", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}], ",", "1"}], "]"}], ",", 
     RowBox[{"f", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}], ",", 
       RowBox[{"-", "1"}]}], "]"}], ",", 
     RowBox[{"f", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"3", ",", 
         RowBox[{"-", "4"}]}], "}"}], ",", "1"}], "]"}]}], "}"}],
   FontWeight->"Bold"]]],
 StyleBox["   ",
  FontSlant->"Italic"],
 StyleBox["[2 Marks]", "ExerciseMark"]
}], "ItemNumbered",
