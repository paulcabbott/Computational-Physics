Cell[TextData[{
 "The shapes, at large time are in general \[OpenCurlyQuote]affinely regular \
polygons\[CloseCurlyQuote].  When ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "4"}], TraditionalForm]]],
 ", these are parallelograms. Continuing with our normalisation convention of \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sum]", 
     SubscriptBox["z", "i"]}], "=", "0"}], TraditionalForm]]],
 ", a parallelogram is defined by two vertices ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "1"], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "2"], TraditionalForm]]],
 " with the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["3", "rd"], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["4", "th"], TraditionalForm]]],
 " vertices are ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    SubscriptBox["z", "1"]}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    SubscriptBox["z", "2"]}], TraditionalForm]]],
 " respectively."
}], "Text",
 Editable->False,
 Deletable->False,
 CellChangeTimes->{{3.451700920453753*^9, 3.451701050630549*^9}, {
  3.451701215528797*^9, 3.451701216378221*^9}}],

Cell[TextData[{
 "After a time ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]]],
 ", our intial parallelogram has evolved to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["z", "\[RightVector]"], "(", "t", ")"}], "=", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "t"}], " ", "A"}]], "\[CenterDot]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["z", "1"], ",", 
        SubscriptBox["z", "2"], ",", 
        RowBox[{"-", 
         SubscriptBox["z", "1"]}], ",", 
        RowBox[{"-", 
         SubscriptBox["z", "2"]}]}], ")"}], "\[Transpose]"}]}]}], 
   TraditionalForm]]]
}], "Text",
