      RowBox[{"0", ",", "0"}], "}"}]}], ",", 
    RowBox[{"PlotRange", "\[Rule]", 
     RowBox[{"1.1", 
      RowBox[{"Max", "[", 
       RowBox[{"Abs", "/@", "lst"}], "]"}], 
