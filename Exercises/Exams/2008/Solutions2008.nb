Cell[CellGroupData[{

Cell[TextData[{
 "(",
 
 CounterBox["LetteredPart",
  CounterFunction:>(Part[
    CharacterRange["a", "z"], #]& )],
 ") ",
 "Denote the roots of the cubic for a given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalE]", "(", 
    RowBox[{"a", ",", "0"}], ")"}], TraditionalForm]]],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    SubscriptBox["a", "s"]}], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "x"], TraditionalForm]]],
 ". When there are 3 real roots, it can be shown (but you need not in this \
exam) that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"-", "1"}], "/", "2"}], "<", 
    RowBox[{"-", 
     SubscriptBox["a", "s"]}], "<", "0", "<", 
    SubscriptBox["a", "n"], "<", "1", "<", 
    SubscriptBox["a", "x"]}], TraditionalForm]]],
 ". The period of the closed orbit for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "<", 
    SubscriptBox["a", "s"], "<", 
    SubscriptBox["a", "x"]}], TraditionalForm]]],
 " is given by ",
 Cell[BoxData[
  RowBox[{"period", "[", 
   SubscriptBox["a", "s"], "]"}]]],
 " where"
}], "LetteredPart",
 CellGroupingRules->{"SectionGrouping", 70},
 CellChangeTimes->{{3.421404838435493*^9, 3.421404914893804*^9}, {
   3.421674311172171*^9, 3.4216743117132*^9}, 3.421674493740282*^9, {
   3.421674541886963*^9, 3.4216745719965353`*^9}, {3.421674646021345*^9, 
   3.421674681697016*^9}, {3.4216754729441223`*^9, 3.421675505967862*^9}, 
   3.421677726875017*^9, {3.421677835244467*^9, 3.4216778513270273`*^9}, 
   3.4217249489246197`*^9, 3.421725001319996*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"period", "[", "a_", "]"}], ":=", 
  RowBox[{"-", 
   FractionBox[
    RowBox[{"2", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"4", " ", 
        SuperscriptBox["a", "3"], " ", 
        RowBox[{"EllipticK", "[", 
         FractionBox[
          RowBox[{"2", " ", 
           SqrtBox[
            RowBox[{"1", "-", 
             RowBox[{"8", " ", 
              SuperscriptBox["a", "3"]}]}]]}], 
          RowBox[{
           RowBox[{"4", " ", 
            SuperscriptBox["a", "3"]}], "+", 
           SqrtBox[
            RowBox[{"1", "-", 
             RowBox[{"8", " ", 
              SuperscriptBox["a", "3"]}]}]], "+", "1"}]], "]"}]}], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"4", " ", 
           SuperscriptBox["a", "3"]}], "+", 
          SqrtBox[
           RowBox[{"1", "-", 
            RowBox[{"8", " ", 
